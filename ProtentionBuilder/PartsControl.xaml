<UserControl x:Class="Urbbox.AutoCAD.ProtentionBuilder.PartsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Urbbox.AutoCAD.ProtentionBuilder"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="300" Background="White">
    <StackPanel>
        <Label x:Name="label" Content="Lista de Peças" VerticalAlignment="Top" FontSize="16" Padding="10,5" Margin="10,0"/>
        <TreeView x:Name="treeView" Height="390" Margin="10,0,10,15">
        </TreeView>
        <Button x:Name="button" Content="Analisar Peças" Margin="10,0" Height="30" />
        <Button x:Name="button_Copy" Content="Adicionar Peça" Margin="10,10,10,0" Height="30"/>
    </StackPanel>
</UserControl>
